{
  "name": "vitepress-dynamic-sidebar",
  "version": "0.0.1",
  "description": "Automatically creates a sidebar in Vitepress based on the Markdown files in your codebase.",
  "author": "Schalk Venter <venterschalk@gmail.com> (http://schalkventer.me/)",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "license": "ISC",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.module.js",
  "unpkg": "./dist/index.umd.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.modern.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.modern.js"
    }
  },
  "homepage": "https://github.com/schalkventer/vitepress-auto-sidebar#readme",
  "bugs": {
    "url": "https://github.com/schalkventer/vitepress-auto-sidebar/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/schalkventer/vitepress-auto-sidebar.git"
  },
  "scripts": {
    "build": "microbundle --target node"
  },
  "devDependencies": {
    "microbundle": "^0.15.1",
    "typescript": "^5.9.3",
    "vitepress": "^1.6.4"
  }
}